v-col xs=\"12\" sm=\"6\">\r\n        <v-card class=\"pa-2 ma-5\" width=\"350px\" shaped>\r\n          <v-card-title>Piscícola</v-card-title>\r\n          <v-card-text>\r\n            <v-combobox\r\n              color=\"#325d79\"\r\n              item-color=\"#325d79\"\r\n              solo\r\n              hide-details\r\n              class=\"mb-5\"\r\n              label=\"Seleccionar Piscícola\"\r\n              :items=\"piscicolas\"\r\n              @change=\"() => updateEstanques()\"\r\n              v-model=\"id_piscicolas\"\r\n            ></v-combobox>\r\n            <p class=\"pb-1\">Ubicación: {{ selectedPiscicola.ubicacion }}</p>\r\n            <p>Descripción: {{ selectedPiscicola.descripcion }}</p>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col xs=\"12\" sm=\"6\">\r\n        <v-card class=\"pa-2 ma-5\" width=\"350px\" shaped>\r\n          <v-card-title>Estanque</v-card-title>\r\n          <v-card-text class=\"mt-2\">\r\n            <v-combobox\r\n              color=\"#325d79\"\r\n              item-color=\"#325d79\"\r\n              solo\r\n              hide-details\r\n              class=\"mb-12\"\r\n              label=\"Seleccionar\"\r\n              :items=\"estanques\"\r\n              @change=\"() => selectedItemEstanque()\"\r\n              v-model=\"id_estanque\"\r\n            ></v-combobox>\r\n            <p>Descripción: {{ selectedEstanque.descripcion }}</p>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col class=\"text-center\" cols=\"12\">\r\n        <v-btn color=\"#325d79\" outlined @click=\"updateGraphs\">Visualizar</v-btn>\r\n      </v-col>\r\n\r\n      <v-col class=\"text-center\" cols=\"6\">\r\n        <line-chart\r\n          v-if=\"loaded\"\r\n          :chartdata=\"chartData\"\r\n          :options=\"chartOptions\"\r\n        />\r\n      </v-col>\r\n\r\n      <v-col class=\"text-center\" cols=\"6\">\r\n        <line-chart\r\n          v-if=\"loaded\"\r\n          :chartdata=\"chartData\"\r\n          :options=\"chartOptions\"\r\n        />\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { postRequest } from '@/plugins/api.js'\r\nimport LineChart from '@/components/Chart.vue'\r\n\r\nexport default {\r\n  name: 'LineChartContainer',\r\n  components: { LineChart },\r\n  created() {\r\n    this.updatePiscicolas()\r\n  },\r\n  data() {\r\n    return {\r\n      piscicolas: [],\r\n      piscicolasfull: [],\r\n      selectedPiscicola: {\r\n        descripcion: '',\r\n        id_piscicolas: null,\r\n        id_usuario: null,\r\n        nombre_piscicola: '',\r\n        ubicacion: ''\r\n      },\r\n      estanques: [],\r\n      estanquesfull: [],\r\n      selectedEstanque: {\r\n        id_estanque: null,\r\n        id_piscicolas: null,\r\n        nombre_estanque: '',\r\n        descripcion: ''\r\n      },\r\n      id_piscicolas: '',\r\n      id_estanque: '',\r\n      dataCalculadafull: [],\r\n      chartData: {\r\n        labels: [],\r\n        datasets: [{ label: 'Sensor 1', backgroundColor: '#f87979', data: [] }]\r\n      },\r\n      chartOptions: null,\r\n      loaded: false\r\n    }\r\n  },\r\n  async mounted() {\r\n    this.updateGraphs()\r\n  },\r\n  methods: {\r\n    selectedItemPiscicola() {\r\n      this.selectedPiscicola = this.piscicolasfull.find(\r\n        x => x.id_piscicolas === this.id_piscicolas.value\r\n      )\r\n      //this.showDialogPiscicola = true\r\n    },\r\n    selectedItemEstanque() {\r\n      this.selectedEstanque = this.estanquesfull.find(\r\n        x => x.id_estanque === this.id_estanque.value\r\n      )\r\n      console.log('selecEstanque---->')\r\n      console.log(this.selectedEstanque)\r\n      //this.showDialogEstanque = true\r\n    },\r\n    updatePiscicolas() {\r\n      postRequest('piscicolas').then(response => {\r\n        if (response.data.res == 200) {\r\n          this.piscicolas = response.data.data.map(piscicola => ({\r\n            text: piscicola.nombre_piscicola,\r\n            value: piscicola.id_piscicolas\r\n          }))\r\n          this.piscicolasfull = response.data.data\r\n        } else alert(response.data.msg)\r\n      })\r\n    },\r\n    updateEstanques() {\r\n      console.log('--->>>')\r\n      console.log(this.id_piscicolas)\r\n      if (this.id_piscicolas.value !== null) {\r\n        postRequest('estanques', {\r\n          id_piscicolas: this.id_piscicolas.value\r\n        }).then(response => {\r\n          if (response.data.res == 200) {\r\n            this.estanques = response.data.data.map(estanque => ({\r\n              text: estanque.nombre_estanque,\r\n              value: estanque.id_estanque\r\n            }))\r\n            this.estanquesfull = response.data.data\r\n            this.selectedItemPiscicola()\r\n          }\r\n        })\r\n      } else alert('Seleccione una Piscicola')\r\n    },\r\n    updateDataCalculada() {\r\n      console.log('id_estanque ------>')\r\n      console.log(this.id_estanque.value)\r\n      postRequest('data_calculada', {\r\n        id_estanque: this.id_estanque.value,\r\n        initDate: this.initDate,\r\n        finalDate: this.finalDate\r\n      }).then(response => {\r\n        if (response.data.res == 200) {\r\n          this.dataCalculadafull = response.data.data\r\n          console.log('datcalculada obtenidad del back ------>')\r\n          console.log(this.dataCalculadafull)\r\n        } else alert(response.data.msg)\r\n      })\r\n    },\r\n    updateGraphs() {\r\n      console.log('id_estanque ------>')\r\n      console.log(this.id_estanque.value)\r\n      postRequest('graphs', {\r\n        id_estanque: this.id_estanque.value,\r\n        initDate: this.initDate,\r\n        finalDate: this.finalDate\r\n      }).then(response => {\r\n        if (response.data.res == 200) {\r\n          console.log('mediciones obtenidad del back------>')\r\n          console.log(response.data.data)\r\n          this.chartData.datasets[0].data = response.data.data.map(\r\n            chartDatas => chartDatas.dato\r\n          )\r\n          this.chartData.labels = response.data.data.map(\r\n            chartDatas => chartDatas.fecha\r\n          )\r\n\r\n          //this.chartData = this.datasetsx\r\n\r\n          this.loaded = true\r\n          console.log('chardata------>')\r\n          console.log(this.chartData)\r\n        } else alert(response.data.msg)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"]}]}